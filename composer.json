{
    "name": "thomasez/aceeditor-bundle",
    "replace": {
        "norzechowicz/aceeditor-bundle": "dev-master"
    },
    "type": "symfony-bundle",
    "description": "Bundle that integrate excellent JavaScript ace editor into Symfony2 Form.",
    "keywords": ["ace", "editor", "symfony", "form", "bundle", "javascript"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "twig/twig": "~1.2",
        "symfony/framework-bundle": "~2.8|~3.0"
    },
    "autoload": {
        "psr-0": { "Norzechowicz\\AceEditorBundle\\": "" }
    },
    "target-dir" : "Norzechowicz/AceEditorBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
