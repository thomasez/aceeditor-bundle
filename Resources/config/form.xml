<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="norzechowicz_ace_editor.form.type.ace_editor" class="Norzechowicz\AceEditorBundle\Form\Extension\AceEditor\Type\AceEditorType">
            <tag name="form.type" alias="ace_editor" />
        </service>
        <service id="norzechowicz_ace_editor.form.type.jsoneditor" class="Norzechowicz\AceEditorBundle\Form\Extension\AceEditor\Type\JsonEditorType">
            <tag name="form.type" alias="jsoneditor" />
        </service>
        <service id="norzechowicz_ace_editor.form.type.flexijsoneditor" class="Norzechowicz\AceEditorBundle\Form\Extension\AceEditor\Type\FlexiJsonEditorType">
            <tag name="form.type" alias="flexijsoneditor" />
        </service>
    </services>
</container>
